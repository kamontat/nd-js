    "@types/fs-extra": "5.1.0",
    "@types/inquirer": "0.0.44",
    "@types/jest": "24.9.0",
    "@types/js-yaml": "3.12.1",
    "@types/jsonwebtoken": "8.3.6",
    "@types/listr": "0.14.2",
    "@types/mock-fs": "3.6.30",
    "@types/mustache": "0.8.32",
    "@types/node": "10.17.13",
    "@types/puppeteer-core": "1.9.0",
    "@types/random-js": "1.0.31",
    "@types/request": "2.48.4",
    "@types/request-promise": "4.1.45",
    "@types/semver": "6.2.0",
    "@types/tmp": "0.1.0",
    "@types/uuid": "3.4.6",
    "@types/vorpal": "1.12.0",
    "@types/yargs": "13.0.5",
    "babel-core": "6.26.3",
    "babel-jest": "24.9.0",
    "cloc": "2.5.1",
    "coveralls": "3.0.9",
    "css-loader": "1.0.1",
    "execa": "1.0.0",
    "gh-pages": "2.2.0",
    "html-loader": "0.5.5",
    "jest": "24.9.0",
    "jest-extended": "0.11.2",
    "jest-junit": "5.2.0",
    "jest-raw-loader": "1.0.1",
    "mock-fs": "4.10.4",
    "pkg": "4.4.2",
    "rimraf": "2.7.1",
    "rxjs-tslint": "0.1.7",
    "terser-webpack-plugin": "1.4.3",
    "tmp": "0.1.0",
    "to-string-loader": "1.1.6",
    "ts-jest": "24.3.0",
    "ts-loader": "5.4.5",
    "tslint": "5.20.1",
    "tslint-loader": "3.5.4",
    "typedoc": "0.16.7",
    "typedoc-plugin-external-module-name": "1.1.3",
    "typedoc-plugin-internal-external": "1.0.10",
    "typedoc-plugin-single-line-tags": "1.0.0",
    "typescript": "3.7.5",
    "uglify-js": "3.7.5",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dashboard": "2.1.0",
    "webpack-node-externals": "1.7.2"
  },
  "scripts": {
    "commit": "gitgo commit",
    "compile": "webpack",
    "compile:dashboard": "webpack-dashboard -- webpack",
    "compile:prod": "webpack --mode=production",
    "compile:watch": "yarn compile:dashboard --watch",
    "start": "node ./.caches/nd.min.js",
    "start:admin": "node ./.caches/nd-admin.min.js",
    "build": "yarn deployment build --only-main",
    "build:admin": "yarn deployment build --only-admin",
    "build:all": "yarn deployment build",
    "build:debug": "yarn clean && yarn build --debug",
    "build:admin:debug": "yarn clean && yarn build:admin --debug",
    "build:all:debug": "yarn clean && yarn build --debug && yarn build:admin --debug",
    "docs": "yarn deployment doc",
    "gh-pages": "yarn docs --only-push",
    "loc": "yarn deployment loc",
    "clean": "rimraf .caches bin coverage docs",
    "clean:all": "yarn clean && rimraf yarn.lock node_modules",
    "deployment": "node ./scripts/deployment.js",
    "deploy:all": "yarn deployment all",
    "deploy:all:ci": "yarn deployment all --ci",
    "analyze": "webpack --json > ./.caches/report.json",
    "analyze:prod": "webpack --mode=production --json > ./.caches/report.json",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "test:token": "yarn start:admin create --fullname \"admin user admin@nd.com\" --username admin --expire 1y --not-before 1ms --version-range \"*\" --json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "coverage": "jest --coverage",
    "coverage:watch": "jest --coverage --watch",
    "coverage:ci": "jest --ci --coverage --verbose --maxWorkers 1"
  },
  "pkg": {
    "assets": [
      "package.json",
      "node_modules/config/**/*.*"
    ]
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/jest.setup.js"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/src/**/__tests__/**/*.ts?(x)",
      "**/src/**/?(*.)+(spec|test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/_*.ts",
      "!src/command/**/*.ts",
      "!src/nd.ts",
      "!src/constants/command.const.ts",
      "!src/constants/option.const.ts",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "html",
      "mustache",
      "json"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.mustache$": "<rootDir>/test/htmlLoader.js",
      "^.+\\.css$": "jest-raw-loader"
    },
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "output": "coverage/reporter/junit/result.xml"
        }
      ]
    ],
    "globals": {
      "COMPILED_DATE": 1543003978
    }
  }
}
